{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/sarif-2.1.0-rtm.5.json",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "SARIF Functional Testing",
          "rules": [
            {
              "id": "JSON0001",
              "name": "SyntaxError",
              "fullDescription": {
                "text": "The schema is not a valid JSON document."
              },
              "messageStrings": {
                "default": {
                  "text": "at \"{0}\": JSON syntax error: {1}"
                }
              },
              "defaultConfiguration": {
                "level": "error"
              }
            }
          ]
        }
      },
      "invocations": [
        {
          "executionSuccessful": true
        }
      ],
      "artifacts": [
        {
          "location": {
            "uri": "FunctionalTestOutput.ValidateCommand/Inputs.JSON1001.SyntaxError.sarif",
            "uriBaseId": "TEST_DIR"
          }
        }
      ],
      "results": [
        {
          "ruleId": "JSON0001",
          "ruleIndex": 0,
          "level": "error",
          "message": {
            "id": "default",
            "arguments": [
              "$schema",
              "Invalid property identifier character: ,. Path '$schema', line 3, position 2."
            ]
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "index": 0
                },
                "region": {
                  "startLine": 3,
                  "startColumn": 2
                }
              }
            }
          ]
        }
      ],
      "columnKind": "utf16CodeUnits"
    }
  ]
}