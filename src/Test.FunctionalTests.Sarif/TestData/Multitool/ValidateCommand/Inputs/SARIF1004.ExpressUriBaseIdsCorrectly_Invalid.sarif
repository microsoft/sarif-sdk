                }
              ]
            }
          ],
          "relatedLocations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "file:///C:/src/file.c",
                  "uriBaseId": "%SRCROOT%"
                }
              }
            }
          ],
          "attachments": [
            {
              "artifactLocation": {
                "uri": "file:///C:/src/file.c",
                "uriBaseId": "%SRCROOT%"
              }
            }
          ],
          "provenance": {
            "conversionSources": [
              {
                "artifactLocation": {
                  "uri": "file:///C:/src/file.c",
                  "uriBaseId": "%SRCROOT%"
                }
              }
            ]
          },
          "fixes": [
            {
              "artifactChanges": [
                {
                  "artifactLocation": {
                    "uri": "file:///C:/src/file.c",
                    "uriBaseId": "%SRCROOT%"
                  },
                  "replacements": [
                    {
                      "deletedRegion": {
                        "charLength": 1
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ],
      "columnKind": "utf16CodeUnits"
    }
  ]
}