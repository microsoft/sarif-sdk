{
  "$schema": "http://json.schemastore.org/sarif-2.0.0-csd.2.beta.2019-04-03",
  "version": "2.0.0-csd.2.beta.2019-04-03",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeScanner",
          "fullName": "CodeScanner for EN-US",
          "version": "2.1.0 optimized release for x86",
          "semanticVersion": "2.1.0",
          "dottedQuadFileVersion": "1.2.3.4",
          "downloadUri": "https://example.com/sensitive_terms.html",
          "globalMessageStrings": {
            "noContent": {
              "text": "This file has no content to review."
            }
          },
          "rules": [
            {
              "id": "TEST1001",
              "messageStrings": {
                "a": {
                  "text": "Review all image content for geopolitically sensitive graphics."
                }
              }
            }
          ],
          "artifactIndices": [
            0
          ]
        }
      },
      "artifacts": [
        {
          "location": {
            "uri": "CodeScanner.exe"
          }
        }
      ],
      "results": [
        {
          "ruleId": "TEST1001",
          "ruleIndex": 0,
          "message": {
            "id": "a"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "file.txt"
                }
              }
            }
          ]
        }
      ],
      "columnKind": "utf16CodeUnits"
    }
  ]
}