{
  "$schema": "http://json.schemastore.org/sarif-2.0.0-csd.2.beta.2018-10-10",
  "version": "2.0.0-csd.2.beta.2018-10-10",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner",
        "semanticVersion": "2.1.0"
      },
      "files": {
        "file.txt": {"mimeType": "text/plain"}
      },
      "results": [
        { "ruleId": "C1001", "message": {"messageId": "default"}, "locations": [{"physicalLocation": {"fileLocation": {"uri": "file.txt"}}}]}
      ],
      "resources": { "rules": { "C1001": { "messageStrings": { "default": "Review all plaintext content for geopolitically sensitive terms." } } } }
    }
  ]
}