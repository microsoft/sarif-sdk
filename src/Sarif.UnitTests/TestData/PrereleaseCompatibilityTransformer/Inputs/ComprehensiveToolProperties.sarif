{
  "$schema": "http://json.schemastore.org/sarif-2.0.0-csd.2.beta.2019-01-09",
  "version": "2.0.0-csd.2.beta.2019-01-09",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner",
        "semanticVersion": "2.1.0",
        "dottedQuadFileVersion": "1.2.3.4",
        "downloadUri": "https://example.com/sensitive_terms.html",
        "sarifLoggerVersion": "1.5.0",
        "fileVersion": "1.2.3.4",
        "fullName": "CodeScanner for EN-US",
        "language": "en-us",
        "version": "2.1.0 optimized release for x86",
        "properties": {
          "tags": [ "singleTag" ],
          "propertyKey": "propertyValue"
        },
        "globalMessageStrings": {
          "noContent": "This file has no content to review."
        },
        "rulesMetadata": [
          {
            "id": "GEOPOL1001",
            "messageStrings": {
              "a": "Review all image content for geopolitically sensitive graphics."
            }
          }
        ],
        "notificationsMetadata": [
          {
            "id": "MSG1001",
            "messageStrings": {
              "a": "Analyze {0}..."
            }
          }
        ]
      },
      "results": [
        {
          "ruleId": "GEOPOL1001",
          "ruleIndex": 1,
          "message": {
            "messageId": "a"
          },
          "locations": [
            {
              "physicalLocation": {
                "fileLocation": {
                  "uri": "file.txt"
                }
              }
            }
          ]
        }
      ],
      "columnKind": "utf16CodeUnits"
    }
  ]
}