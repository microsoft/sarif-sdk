{
  "$schema": "http://json.schemastore.org/sarif-1.0.0",
  "version": "1.0.0",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner",
        "semanticVersion": "2.1.0"
      },
      "files": {
        "file.txt": {
          "mimeType": "text/plain",
          "uriBaseId": "SRCROOT"
        }
      },
      "results": [
        {
          "ruleId": "C1",
          "message": "a",
          "locations": [
            {
              "analysisTarget": {
                "uri": "file.txt",
                "uriBaseId": "SRCROOT"
              },
              "properties": {
                "key": "#SRCROOT#file.txt"
              }
            }
          ]
        }
      ]
    }
  ]
}