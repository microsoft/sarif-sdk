<UserControl x:Class="Microsoft.Sarif.Viewer.Views.SarifToolWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Microsoft.Sarif.Viewer.Views"
             xmlns:converters="clr-namespace:Microsoft.Sarif.Viewer.Converters"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <UserControl.Resources>
        <converters:ObjectToVisibilityConverter x:Key="objectToVisibilityConverter" />
    </UserControl.Resources>
    <Grid>
        <local:CodeLocations Visibility="{Binding Converter={StaticResource objectToVisibilityConverter}}" />
    </Grid>
</UserControl>
