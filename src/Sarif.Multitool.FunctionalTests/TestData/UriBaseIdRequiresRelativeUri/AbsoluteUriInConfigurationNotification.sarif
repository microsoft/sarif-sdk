{
  "$schema": "http://json.schemastore.org/sarif-2.0.0",
  "version": "2.0.0",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner"
      },
      "invocations": [
        {
          "configurationNotifications": [
            {
              "message": {
                "text": "Message."
              },
              "physicalLocation": {
                "fileLocation": {
                  "uriBaseId": "%SRCROOT%",
                  "uri": "file:///c:/src/file.c"
                }
              }
            }
          ]
        }
      ]
    }
  ]
}