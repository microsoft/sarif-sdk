{
  "$schema": "http://json.schemastore.org/sarif-2.0.0",
  "version": "2.0.0",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner"
      },
      "results": [
        {
          "ruleId": "TST0001",
          "level": "error",
          "fixes": [
            {
              "fileChanges": [
                {
                  "fileLocation": {
                    "uriBaseId": "%SRCROOT%",
                    "uri": "file:///c:/src/file.c"
                  },
                  "replacements": [
                    {
                      "deletedRegion": {
                        "charOffset": 0,
                        "charLength": 1
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}