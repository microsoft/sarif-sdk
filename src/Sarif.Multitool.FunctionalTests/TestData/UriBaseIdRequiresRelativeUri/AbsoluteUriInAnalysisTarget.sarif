{
  "$schema": "http://json.schemastore.org/sarif-2.0.0",
  "version": "2.0.0",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner"
      },
      "results": [
        {
          "ruleId": "TST0001",
          "level": "error",
          "analysisTarget": {
            "uri": "file:///c:/src/file.c",
            "uriBaseId": "%SRCROOT%"
          }
        }
      ]
    }
  ]
}