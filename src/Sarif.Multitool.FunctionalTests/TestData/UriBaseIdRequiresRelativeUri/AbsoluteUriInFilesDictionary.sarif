{
  "$schema": "http://json.schemastore.org/sarif-2.0.0",
  "version": "2.0.0",
  "runs": [
    {
      "tool": {
        "name": "CodeScanner"
      },
      "files": {
        "/c:/src/file.c": {
          "fileLocation": {
            "uri": "file:///c:/src/file.c",
            "uriBaseId": "%SRCROOT%"
          }
        }
      }
    }
  ]
}